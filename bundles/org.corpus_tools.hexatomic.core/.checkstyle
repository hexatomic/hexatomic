<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="true" sync-formatter="false">
  <local-check-config name="Hexatomic Coding Style" location="/hexatomic/checkstyle.xml" type="project" description="Hexatomic Coding Style, which is derived from the Google Java Style Guide (https://google.github.io/styleguide/javaguide.html)">
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="Hexatomic Coding Style" local="true">
    <file-match-pattern match-pattern="." include-pattern="true"/>
  </fileset>
</fileset-config>
